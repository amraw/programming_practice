package DP;

public class matrixChainMultiplication {
	
int matrixMultiplicationRecur(int []p, int i, int j)
{
	int k;
	int min = Integer.MAX_VALUE;
	if(i==j)
		return 0;
	
	for(k=i; k<j; k++)
	{
		int multiplyCount = matrixMultiplicationRecur(p,i,k)+matrixMultiplicationRecur(p,k+1,j)+
							
	}
	
	return min;
}

}
